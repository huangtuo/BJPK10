<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:zhy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/selectPlan_tv_series"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/common_size_20"
        android:layout_marginTop="@dimen/common_size_10"
        android:text="定位系列"
        android:textSize="@dimen/text_size_14"
        android:textColor="@color/text_color_black04"
        />

    <com.rx.mvp.zyzj.widget.flowLayout.TagFlowLayout
        android:id="@+id/selectPlan_flowlayout_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/common_size_10"
        zhy:max_select="-1"
        />


</LinearLayout>
